};






			
		pop1.on("ended",function(){
				if(current==1){
							ClearFlags();							
				}});
				
		$.each(wordTimes1, function(id, time) {
								pop1.footnote({
									start: time.start,
									end: time.end,
									text: '',
									target: id,
									effect: "applyclass",
									applyclass: "selected1"
								});
		});		
		
