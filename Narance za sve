		// funkcija za avtomatsko pustanje na intro-audio pri uklucuvanje na stranata
		function playIntro(){
			var snd=document.getElementById('intro');
			canPlayMP3 = (typeof snd.canPlayType === "function" && snd.canPlayType("audio/mpeg") !== "");
			snd.src=canPlayMP3?'audio/Intro.mp3':'audio/Intro.ogg';
			snd.load();
			snd.play();	
		}
				function stopIntro(){
			var snd=document.getElementById('intro');
			canPlayMP3 = (typeof snd.canPlayType === "function" && snd.canPlayType("audio/mpeg") !== "");
			snd.src=canPlayMP3?'audio/Intro.mp3':'audio/Intro.ogg';
			snd.load();
			snd.pause();
			snd.currentTime=0;
		}

		// funkcii za sync na tekstot i audioto za soodvetnata strana pri klik na play/pause so koristenje na plugin popcorn.js	
		function ClearFlags(){
			ppFlag=1;
			flag=1;
		}

		function SetFlags(){
			ppFlag=0;
			flag=0;
		}		
										
		var makeDelay= function(x){ 
							timeout.stop();
								if(x==0){}
								else{
							timeout = new Timer(function(){ 
								//if(current == 0 || current == 1){
								$('#bb-bookblock').bookblock('next');
								ClearFlags();
							}/*}*/,x*1000);}
		}
		
	
		
