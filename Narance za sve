			//proverka autoplay!
				function checkPlay(){
				if(autoplayFlag==0){
			autoplayFlag = 1;
		
			timeout.stop();
			makeDelay(9.40); //treba da e isto so dolniot delay na koricata
			playIntro(); 
			SetFlags();
			}
			else{ 
			autoplayFlag=0;
			timeout.stop();	
			stopIntro();
			ClearFlags();

			}
		}
		
		$(".autoplay_on").hide(100);
		$(".autoplay_btn").click(function() {
			if(autoplayFlag == 0) {
				$(".autoplay_on").hide(100);
			} else if(autoplayFlag == 1) {
				$(".autoplay_on").show(100);
				}
		});
		
		// JavaScript funckii za animacija na stranite i slikovnica efektot so koristenje na plugin bookblock.js
	
			var Page = (function() {
				
				var config = {
						$bookBlock : $( '#bb-bookblock' ),
						$navNext : $( '#bb-nav-next' ),
						$navPrev : $( '#bb-nav-prev' ),
						$replay : $( '#rplBtn' ),
						$navLast : $( '#bb-nav-last' ),
					},
					
					init = function() {
						config.$bookBlock.bookblock( {
							speed : 1000,
							shadowSides : 0.8,
							shadowFlip : 0.4,
							onBeforeFlip : function(page) { flipSound(); },
							onEndFlip : function(old, page, isLimit){
							
								current = page;
								timeout.stop();
								if(autoplayFlag==1){							
									switch(current){
									case 0: playIntro(); makeDelay(9.40); SetFlags(); break;//da se smeni uste ednas gore!
									//case 1: makeDelay(1.55);  break;
									case 1: Timer(function(){pop1.play();},500); makeDelay(4.52+2.00); SetFlags(); break;
									case 2: Timer(function(){pop2.play();},500); makeDelay(17.25+2.00); SetFlags(); break;
									case 3: Timer(function(){pop3.play();},500); makeDelay(14.95+2.00); SetFlags(); break;		
									case 4: Timer(function(){pop4.play();},500); makeDelay(9.62+2.00); SetFlags(); break;		
									case 5: Timer(function(){pop5.play();},500); makeDelay(21.56+2.00); SetFlags(); break;	
									case 6: Timer(function(){pop6.play();},500); makeDelay(11.08+2.00); SetFlags(); break;
									case 7: Timer(function(){pop7.play();},500); makeDelay(7.90+2.00); SetFlags(); break;
									case 8: Timer(function(){pop8.play();},500); makeDelay(18.81+2.00); SetFlags(); break;
									case 9: Timer(function(){pop9.play();},500); makeDelay(15.55+2.00); SetFlags(); break;
									case 10: Timer(function(){pop10.play();},500); makeDelay(14.30+2.00); SetFlags(); break;
									case 11: Timer(function(){pop11.play();},500); makeDelay(10.80+2.00); SetFlags(); break;
									case 12: Timer(function(){pop12.play();},500); makeDelay(10.25+2.00); SetFlags(); break;
									case 13: Timer(function(){pop13.play();},500); makeDelay(3.30+2.00); SetFlags(); break;
									case 14: Timer(function(){pop14.play();},500); makeDelay(7.80+2.00); SetFlags(); break;
									case 15: Timer(function(){pop15.play();},500);  makeDelay(7.70+1.00);SetFlags(); break;
									} //switch
								}//autplay check
							} //onendflip
						});
						initEvents();
					},
					initEvents = function() {
						var $slides = config.$bookBlock.children();
						// add navigation events
						config.$navNext.on( 'click touchstart', function() {
							config.$bookBlock.bookblock( 'next' );
							/////////////////////////////////////////////////////////////////
							var allaudio = document.getElementsByClassName('audio');
							for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
							/////////////////////////////////////////////////////////////////
							return false;
						} );
						config.$navPrev.on( 'click touchstart', function() {
							config.$bookBlock.bookblock( 'prev' );
							/////////////////////////////////////////////////////////////////
							var allaudio = document.getElementsByClassName('audio');
							for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
							////////////////////////////////////////////////////////////////
							return false;
						} );
						config.$replay.on( 'click touchstart', function() {
							config.$bookBlock.bookblock( 'first' );
							/////////////////////////////////////////////////////////////////
							var allaudio = document.getElementsByClassName('audio');
							for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
							/////////////////////////////////////////////////////////////////
							return false;
						} );
						config.$navLast.on( 'click touchstart', function() {
							config.$bookBlock.bookblock( 'last' );
							/////////////////////////////////////////////////////////////////
							var allaudio = document.getElementsByClassName('audio');
							for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
							/////////////////////////////////////////////////////////////////
							return false;
						} );
						// add swipe events
						$slides.on( {
							'swipeleft' : function( event ) {
								config.$bookBlock.bookblock( 'next' );
								/////////////////////////////////////////////////////////////////
								var allaudio = document.getElementsByClassName('audio');
								for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
								/////////////////////////////////////////////////////////////////
								return false;
							},
							'swiperight' : function( event ) {
								config.$bookBlock.bookblock( 'prev' );
								/////////////////////////////////////////////////////////////////
								var allaudio = document.getElementsByClassName('audio');
								for( var i=0; i<allaudio.length; i++)
								{
									allaudio[i].pause();
									allaudio[i].src = allaudio[i].src;
								}
								/////////////////////////////////////////////////////////////////
								return false;
							}
						} );
						// add keyboard events
						$( document ).keydown( function(e) {
							var keyCode = e.keyCode || e.which,
								arrow = {
									left : 37,
									up : 38,
									right : 39,
									down : 40,
								};
							switch (keyCode) {
								case arrow.left:
									config.$bookBlock.bookblock( 'prev' );
									////////////////////////////////////////////////////////////////
									var allaudio = document.getElementsByClassName('audio');
									for( var i=0; i<allaudio.length; i++)
									{
										allaudio[i].pause();
										allaudio[i].src = allaudio[i].src;
									}
									////////////////////////////////////////////////////////////////
									break;
								case arrow.right:
									config.$bookBlock.bookblock( 'next' );
									////////////////////////////////////////////////////////////////
									var allaudio = document.getElementsByClassName('audio');
									for( var i=0; i<allaudio.length; i++)
									{
										allaudio[i].pause();
										allaudio[i].src = allaudio[i].src;
									}
									////////////////////////////////////////////////////////////////
									break;
							}
						} );
					};
					return { init : init };
			})();
		</script>
		
		<script>
				Page.init();		
		</script>
		
	</body>
</html>
