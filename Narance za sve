		function PlayPause0() {
			var intro = document.getElementById("intro");
			if( ppFlag == 1 ) { 
				intro.play(); 
				ppFlag = 0; 
			} else if( ppFlag == 0 ) { 
				intro.pause(); 
				ppFlag = 1; 
			}
		}
			
		//funkcija koja se aktivira pri klik vrz slonceto za play/pause za prvata stranata potoa sledi delot so sync na zbor/audio za prva strana (popcorn.js) za site strani.
		//PRVA STRANA!
		function PlayPause1() { 
			if( ppFlag == 1 ) { 
				pop1.play();  
			} else if( ppFlag == 0 ) { 
				pop1.pause(); 
			} 
		}
		
		var pop1 = Popcorn("#prva_strana");
var wordTimes1 = {
"w1-1": { start: 0.02, end: 1.61 },//Jednoga
