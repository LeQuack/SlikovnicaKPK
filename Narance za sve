		function flipSound() {
			document.getElementById("flip").play();
		}
		
		// funkcija koja gi trga kopcinjata ako rezolucijata na ekranot e pomala od 800x600 (telefoni - se da raboti na TAP i SLIDE) 
		$(window).resize( function phoneMode() {
			if ( ($(window).width() < 800) && ($(window).height() < 600) ) {
				$("#mainNav").hide();
				$(".play_pause").hide();
				document.getElementById("autoplayOn").style.top="-70%";
				document.getElementById("autoplayOn").style.left="-15%";
				document.getElementById("autoplayOn").style.width="60px";
				document.getElementById("autoplayOn").style.height="60px";
			}
			else
				{
					$("#mainNav").show();
					$(".play_pause").show();
					document.getElementById("autoplayOn").style.top="-10%";
					document.getElementById("autoplayOn").style.left="-30%";
					document.getElementById("autoplayOn").style.width="120px";
					document.getElementById("autoplayOn").style.height="120px";
				}
		});
		
		//FLAGS
		var autoplayFlag=0;
		var flag = 1;
		var current = 0;
		var ppFlag = 1;	// 1 e pauzirano, 0 e playing (GLOBALNA ZA SITE)
		//FLAGS
		
			timeout = new Timer(function(){},0);
		
			function Timer(callback, delay) {
			var timerId, start, remaining = delay;
			
			this.stop = function() {
			window.clearTimeout(timerId); 
			remaining = 0; }
			
			this.pause = function() {
				window.clearTimeout(timerId);
				remaining -= new Date() - start;
			};

			this.resume = function() {
				start = new Date();
				timerId = window.setTimeout(callback, remaining);
			};

			this.resume();
			}
			
			function resumeTimer(){
			if (autoplayFlag==1){
				timeout.resume();
				}			
			}
			
			function pauseTimer(){
				if (autoplayFlag==1){
				timeout.pause();
				}			
			}
			
				//gi podesuva fontovite za browseri
		/*if(/chrom(e|ium)/.test(navigator.userAgent.toLowerCase())){
		$(".text").css({"font-family": "SkolaSansCnOffc"});
		}
		
		if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ //test for Firefox/x.x or Firefox x.x (ignoring remaining digits);
		
		$(".text").css({"font-family": "SkolaSansCn"});
		}*/
		// onclick funckii na div "bb-item" za play/pause (touch na mobilni uredi - da nema kopcinja)
		function initTouch() {
			var intro = document.getElementById("intro");
			if ( ppFlag == 1 ) { 
				intro.play(); 
				ppFlag = 0; 
				resumeTimer();
			} else if ( ppFlag == 0 ) { 
				intro.pause(); 
				ppFlag = 1; 
				pauseTimer();
			}
		}
		
		function prvaTouch() {
			var intro = document.getElementById("prva_strana");
			if ( ppFlag == 1 ) { 
				intro.play(); 
				ppFlag = 0; 
				resumeTimer();

			} else if ( ppFlag == 0 ) { 
				intro.pause(); 
				ppFlag = 1; 
				pauseTimer();
			}
		} 
		
